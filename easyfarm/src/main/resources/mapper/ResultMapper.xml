   <select id="getEtcPayResult" resultMap="resultEtcPayMap" parameterType="String" >
		SELECT
			p.etc_pay_code
			,p.plan_workphase_code
			,p.plan_workphase_cate_code
			,p.etc_pay_date
			,p.etc_pay
			,p.etc_pay_detail
			,p.reg_member_id
			,p.reg_date
			,p.client_code
			,r.etc_pay_run_code
			,r.etc_pay_run_date
			,r.etc_pay_run_pay
			,r.etc_pay_run_memo
			,r.complete_status
			,r.reg_member_id
			,r.reg_date
			,r.client_code
		FROM
		tb_etc_pay_run AS r
		JOIN
		tb_exp_etc_pay AS p
		ON
		r.etc_pay_code=p.etc_pay_code
		WHERE
		p.plan_workphase_code = #{workPhaseCode}
   </select>
  
