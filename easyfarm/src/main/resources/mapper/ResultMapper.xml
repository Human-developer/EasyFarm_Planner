  	<select id="getWorkPhaseByProjectCode" resultType="Map" parameterType="String">
		SELECT 
			pw.project_workphase_code 	AS WorkPhaseCode
			,cp.workphase_name 			AS WorkPhaseName
		FROM tb_project_workphase AS pw
		INNER JOIN
		tb_crop_phase_info AS cp
		ON
		pw.crop_phase_info_code = cp.crop_phase_info_code
		WHERE
			pw.project_code =  #{projectCode} 
  	</select>
  	
  	<select id="getProjectNameByFarmName" resultType="Map" parameterType="String">
		SELECT
			p.project_code AS projectCode
			,p.project_name As projectName
		FROM
			tb_farm_project AS p
		where
			p.farm_code = #{farmCode} 
  	</select>
  
  	
  	<select id="getFarmName" resultType="Map" parameterType="String">
		SELECT
			f.farm_name AS farmName
			,f.farm_code AS farmCode
		FROM 
			tb_farm_member AS m
		inner JOIN
			tb_farm AS f
		on
			m.farm_code=f.farm_code
		WHERE
			m.farm_member_id = #{memberId} 
  	</select>
  	
  	
